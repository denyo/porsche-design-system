{
  "name": "@porsche-design-system/ag-grid",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scope": "@porsche-design-system",
  "sideEffects": false,
  "scripts": {
    "build": "PDS_IS_STAGING=${PDS_IS_STAGING:-1} yarn clean:build && yarn build:bundle",
    "build:bundle": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "build-prod": "PDS_IS_STAGING=0 yarn build",
    "test": "yarn build && yarn test:unit",
    "test:unit": "vitest --config tests/unit/config/vitest.config.ts",
    "clean:build": "rm -rf ./dist"
  },
  "devDependencies": {
    "@porsche-design-system/shared": "0.0.0",
    "ag-grid-community": "33.0.4"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
